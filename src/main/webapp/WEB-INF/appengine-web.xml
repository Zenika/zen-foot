<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>zenfoot-prod</application>
    <version>1-3</version>
    <threadsafe>true</threadsafe>


    <static-files>
        <include path="/**.xml" expiration="5m"></include>
        <include path="/**.js" expiration="5m"></include>
        <include path="/**.html" expiration="5m"></include>
        <include path="/**.css" expiration="5m"></include>
        <include path="/img/**" expiration="5m"></include>
        <exclude path="/node_modules/**"></exclude>
    </static-files>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>
</appengine-web-app>
